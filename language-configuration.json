{
    "comments": {
        "lineComment": "#",
        "blockComment": [
            "#",
            ""
        ]
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ]
    ],
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*(-\\s+)?.*:[ ]*$|^\\s*-[ ]*$|^\\s*(steps|modules|tests|then|else|with|properties|args)\\s*:",
        "decreaseIndentPattern": "^\\s*\\}\\s*$"
    },
    "folding": {
        "markers": {
            "start": "^\\s*#region\\b",
            "end": "^\\s*#endregion\\b"
        }
    },
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
}
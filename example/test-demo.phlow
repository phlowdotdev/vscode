tests: 
  - describe: Basic Arithmetic Tests
    main:
      total: 2
    payload: 10
    assert_eq: Total is 20

  - describe: String Concatenation Tests     
    main:
      total: 3
    payload: 5
    assert: !phs payload == "Total is 15"

steps:
  - payload: !phs main.total * payload 
  - payload: !phs `Total is ${payload}`
